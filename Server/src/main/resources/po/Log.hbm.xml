<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.logpo.LogPO" table="log" schema="salesystem">
        <id name="time">
            <column name="time" sql-type="varchar(255)"/>
        </id>
        <property name="id">
            <column name="operator" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="event">
            <column name="event" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>