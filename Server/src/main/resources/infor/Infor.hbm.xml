<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="infor.Infor" table="infor">
        <id name="inforID">
            <column name="inforID" sql-type="long" not-null="true"/>
            <generator class="native"></generator>
        </id>
        <property name="checkerTime">
            <column name="checkerTime" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="checkerID">
            <column name="checkerID" sql-type="varchar(255)" not-null="true"/>
        </property>
    <property name="remark">
        <column name="remark" sql-type="varchar(255)" not-null="true"/>
    </property>
    <property name="result">
        <column name="result" sql-type="varchar(255)" not-null="true"/>
    </property>
</class>
</hibernate-mapping>