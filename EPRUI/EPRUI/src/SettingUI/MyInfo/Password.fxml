<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettingUI.MyInfo.PasswordController">
   <children>
      <VBox onMouseClicked="#Back" prefHeight="400.0" prefWidth="480.0" style="-fx-background-color: #ffffff;" AnchorPane.leftAnchor="0.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="1.0">
               <children>
                  <HBox fx:id="originV" onMouseClicked="#Clicked1" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-border-color: #f2f2f2; -fx-border-width: 2;">
                     <children>
                        <ImageView fitHeight="35.0" fitWidth="30.0" onMouseClicked="#Clicked1">
                           <image>
                              <Image url="@password.png" />
                           </image>
                        </ImageView>
                        <TextField fx:id="origin" focusTraversable="false" onMouseClicked="#Clicked1" prefWidth="260.0" promptText="请输入原始密码" style="-fx-background-color: #ffffff;">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets left="10.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="passV1" onMouseClicked="#Clicked2" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-border-color: #f2f2f2; -fx-border-width: 2;">
                     <children>
                        <ImageView fitHeight="35.0" fitWidth="30.0" onMouseClicked="#Clicked2">
                           <image>
                              <Image url="@password.png" />
                           </image>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </ImageView>
                        <PasswordField fx:id="newpass" focusTraversable="false" onMouseClicked="#Clicked2" prefWidth="260.0" promptText="请输入新密码" style="-fx-background-color: #ffffff;">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </PasswordField>
                     </children>
                     <padding>
                        <Insets left="10.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox fx:id="passV2" onMouseClicked="#Clicked3" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-border-color: #f2f2f2; -fx-border-width: 2;">
                     <children>
                        <ImageView fitHeight="35.0" fitWidth="30.0" onMouseClicked="#Clicked3">
                           <image>
                              <Image url="@password.png" />
                           </image>
                        </ImageView>
                        <TextField fx:id="assure" focusTraversable="false" onMouseClicked="#Clicked3" prefWidth="260.0" promptText="请再次输入新密码" style="-fx-background-color: #ffffff;">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </TextField>
                     </children>
                     <padding>
                        <Insets left="10.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets right="20.0" top="20.0" />
               </padding>
               <VBox.margin>
                  <Insets left="120.0" />
               </VBox.margin>
            </VBox>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Button fx:id="change" mnemonicParsing="false" onMouseClicked="#ClickedChange" prefHeight="50.0" prefWidth="340.0" style="-fx-background-color: #660099;" text="确 认 修 改" textFill="#f2f2f2">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="120.0" right="20.0" />
               </padding>
            </HBox>
            <VBox prefHeight="50.0" prefWidth="400.0" style="-fx-background-color: #ffffff;">
               <padding>
                  <Insets left="120.0" top="5.0" />
               </padding>
               <children>
                  <Label fx:id="attention" alignment="CENTER" prefWidth="340.0" text="Label" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
