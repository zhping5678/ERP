<HBox prefHeight="50.0" prefWidth="350.0" spacing="10.0" style="-fx-background-color: #f2f2f2;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Occupations.searchController">
   <children>
      <Button fx:id="NewB" alignment="CENTER" minHeight="35.0" mnemonicParsing="false" onMouseClicked="#ClickedA" prefHeight="49.0" prefWidth="38.0" style="-fx-background-color: #f2f2f2;" text="+" textFill="#660099">
         <font>
