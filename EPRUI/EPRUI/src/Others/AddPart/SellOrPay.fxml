<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="180.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Others.AddPart.SellOrPayController">
   <children>
      <VBox prefHeight="180.0" prefWidth="400.0" style="-fx-background-color: #ffffff; -fx-border-color: #660099;">
         <children>
            <HBox prefHeight="50.0" prefWidth="400.0">
               <children>
                  <Label fx:id="title" prefHeight="50.0" prefWidth="350.0" text="新 建 客 户" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="close" onMouseClicked="#ClickedClose" onMouseEntered="#EnteredClose" onMouseExited="#ExitedClose" prefHeight="50.0" text="X" textFill="#f2f2f2">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </HBox>
            <AnchorPane prefHeight="1.0" prefWidth="200.0">
               <children>
                  <Line endX="400.0" stroke="#f2f2f2" />
               </children>
            </AnchorPane>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="10.0">
               <padding>
                  <Insets left="100.0" top="7.0" />
               </padding>
               <children>
                  <Button fx:id="newPay" mnemonicParsing="false" onMouseClicked="#ClickedPay" onMouseEntered="#Entered" onMouseExited="#Exited" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-background-radius: 15; -fx-border-color: #660099; -fx-border-radius: 15;" text="新 建 进 货 商" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button fx:id="newSell" mnemonicParsing="false" onMouseClicked="#ClickedSell" onMouseEntered="#Entered" onMouseExited="#Exited" prefWidth="200.0" style="-fx-background-color: #ffffff; -fx-background-radius: 15; -fx-border-color: #660099; -fx-border-radius: 15;" text="新 建 销 售 商" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="100.0" top="10.0" />
               </padding>
            </HBox>
            <Label fx:id="attention" alignment="CENTER" prefWidth="400.0" text="商品ID不能为空" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
