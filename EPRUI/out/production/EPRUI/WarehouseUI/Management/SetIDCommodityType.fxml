<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WarehouseUI.Management.SetIDController">
   <children>
      <VBox prefHeight="180.0" prefWidth="400.0" style="-fx-background-color: #ffffff; -fx-border-color: #660099;">
         <children>
            <HBox prefHeight="50.0" prefWidth="400.0">
               <children>
                  <Label fx:id="title" prefHeight="50.0" prefWidth="350.0" text="新 建 商 品 分 类" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="close" onMouseClicked="#ClickedClose" onMouseEntered="#EnteredClose" onMouseExited="#ExitedClose" prefHeight="50.0" text="X" textFill="#f2f2f2">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </HBox>
            <AnchorPane prefHeight="1.0" prefWidth="200.0">
               <children>
                  <Line endX="400.0" stroke="#f2f2f2" />
               </children>
            </AnchorPane>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label minHeight="35.0" text="新建商品分类ID" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="id" maxWidth="210.0" style="-fx-background-color: #ffffff; -fx-border-color: #f2f2f2;">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
               </children>
               <padding>
                  <Insets left="20.0" top="7.0" />
               </padding>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button fx:id="confirm" mnemonicParsing="false" onMouseClicked="#ClickedConfirm" onMouseEntered="#Entered" onMouseExited="#Exited" style="-fx-background-color: #ffffff; -fx-background-radius: 15; -fx-border-color: #660099; -fx-border-radius: 15;" text="确 定" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="cancel" mnemonicParsing="false" onMouseClicked="#ClickedCancel" onMouseEntered="#Entered" onMouseExited="#Exited" style="-fx-background-color: #ffffff; -fx-background-radius: 15; -fx-border-color: #660099; -fx-border-radius: 15;" text="取 消" textFill="#660099">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="220.0" top="10.0" />
               </padding>
            </HBox>
            <Label fx:id="attention" alignment="CENTER" prefWidth="400.0" text="商品分类ID不能为空" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
